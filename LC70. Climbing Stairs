
// method 1: DP
class Solution {
    public int climbStairs(int n) {
        if(n <= 2) return n;
        int step1 = 1, step2 = 1;
        for(int i=2; i<=n; i++) {
            int tmp = step1 + step2;
            step1 = step2;
            step2 = tmp;
        }
        return step2;
    }
}

// time: O(n)
// space: O(1)


// method 2: DFS

class Solution {
    public int climbStairs(int n) {
        if(n <=2) return n;
        else 
            return climbStairs(n-1) + climbStairs(n-2);
    }
}
